          RowBox[{"EvenQ", "[", 
           RowBox[{"Length", "@", "l1"}], "]"}], "\[Equal]", "True"}], ",", 
         "g", ",", " ", 
         RowBox[{"-", " ", "g"}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"a", "\[Equal]", "1"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"EvenQ", "[", 

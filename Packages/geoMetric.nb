  RowBox[{
   RowBox[{"ncContract", "[", "\[CapitalGamma]", "]"}], "[", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"ncContract", "[", "\[CapitalGamma]", "]"}], "@@", 
     RowBox[{"Most", "@", 
      RowBox[{"{", "x", "}"}]}]}], ",", "\[IndentingNewLine]", 
    RowBox[{"Last", "@", 
     RowBox[{"{", "x", "}"}]}]}], "\[IndentingNewLine]", 

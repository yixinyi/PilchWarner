Cell[CellGroupData[{

Cell["\<\
Text: Christoffel symbols,  spin connection, Riemann tensor, Ricci tensor and \
Ricci scalar\
\>", "Subsection",
 CellChangeTimes->{{3.6527776189114857`*^9, 3.652777649350092*^9}, {
  3.653910909391736*^9, 3.6539109334468822`*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["G", "\[Mu]\[Nu]"], " ", 
      SuperscriptBox["dx", "\[Mu]"], 
      SuperscriptBox["dx", "\[Nu]"]}], "=", 
     RowBox[{
      SubsuperscriptBox["E", "\[Mu]", "a"], 
      SubsuperscriptBox["E", "\[Nu]", "b"], 
      SubscriptBox["\[Eta]", "ab"], 
      SuperscriptBox["dx", "\[Mu]"], 
      SuperscriptBox["dx", "\[Nu]"], " "}]}]}], TraditionalForm]]],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["G", "\[Mu]\[Nu]"], "=", 
    RowBox[{
     SubsuperscriptBox["E", "\[Mu]", "a"], 
     SubsuperscriptBox["E", "\[Nu]", "a"]}]}], TraditionalForm]]],
 "\nIf the metric is expressed in terms of Maurer-Cartan forms (Euclidean):\n\
\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["ds", "2"], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Sigma]", "i"], 
      SuperscriptBox["\[Sigma]", "j"], 
      SubscriptBox["\[Delta]", "ij"]}], "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Sigma]", "\[Mu]"], "i"], 
       SuperscriptBox[
        SubscriptBox["\[Sigma]", "\[Nu]"], "j"], 
       SubscriptBox["\[Delta]", "ij"], 
       SuperscriptBox["dx", "\[Mu]"], 
       SuperscriptBox["dx", "\[Nu]"]}], " ", "\[Rule]", " ", 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Sigma]", "\[Mu]"], "i"], " ", "is", " ", "the", " ", 
       "eigenbein", " "}]}]}]}], TraditionalForm]]],
 "\nStandard expression from Carroll\[CloseCurlyQuote]s book:\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[CapitalGamma]", 
     RowBox[{"\[Mu]", " ", "\[Nu]"}], "\[Lambda]"], "=", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      SuperscriptBox["g", "\[Lambda]\[Rho]"], "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Mu]"], 
        SubscriptBox["g", "\[Rho]\[Nu]"]}], "+", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Nu]"], 
        SubscriptBox["g", "\[Rho]\[Mu]"]}], "-", 
       RowBox[{
        SubscriptBox["\[PartialD]", "\[Rho]"], 
        SubscriptBox["g", "\[Mu]\[Nu]"]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"\[Mu]", " ", "\[Nu]"}], "\[Lambda]"], "=", 
     SubsuperscriptBox["\[CapitalGamma]", 
      RowBox[{"\[Nu]", " ", "\[Mu]"}], "\[Lambda]"]}]}], TraditionalForm]}]],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     SuperscriptBox[
      SubscriptBox["\[CapitalOmega]", "\[Mu]"], "a"], "b"], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["E", "\[Nu]", "a"], 
      SubsuperscriptBox["E", "b", "\[Lambda]"], 
      SubscriptBox[
       SuperscriptBox["\[CapitalGamma]", "\[Nu]"], 
       RowBox[{"\[Mu]", " ", "\[Lambda]"}]]}], "-", 
     RowBox[{
      SubsuperscriptBox["E", "b", "\[Lambda]"], 
      RowBox[{
       SubscriptBox["\[PartialD]", "\[Mu]"], 
       SubsuperscriptBox["E", "\[Lambda]", "a"]}]}]}]}], TraditionalForm]]],
 "\nAntisymmetric properties:  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", 
     RowBox[{"\[Mu]", " ", "a", " ", "b"}]], "=", 
    RowBox[{"-", 
     SubscriptBox["\[CapitalOmega]", 
      RowBox[{"\[Mu]", " ", "b", " ", "a"}]]}]}], TraditionalForm]]],
 "\nRiemann tensor, Ricci tensor and Ricci scalar\n\t",
 Cell[BoxData[{
  FormBox[
   FormBox[
    RowBox[{
     RowBox[{
      SubscriptBox[
       SuperscriptBox["R", "\[Rho]"], 
       RowBox[{"\[Sigma]", " ", "\[Mu]", " ", "\[Nu]"}]], "=", " ", 
      RowBox[{

Cell[TextData[{
 "The Hodge star operator *, in an n-dimensional manifold is a map from \
p-forms to (n \[Minus] p)-forms given by:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"(*", 
    RowBox[{
     SubscriptBox[
      RowBox[{"w", ")"}], 
      RowBox[{
       SubscriptBox["\[Mu]", "1"], "\[Ellipsis]", " ", 
       SubscriptBox["\[Mu]", 
        RowBox[{"n", "-", "p"}]]}]], "\[Congruent]", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"p", "!"}]], 
      SqrtBox[
       RowBox[{"|", "g", "|"}]], 
      SubscriptBox["\[Epsilon]", 
       RowBox[{
        SubscriptBox["\[Mu]", "1"], "\[Ellipsis]", " ", 
        SubscriptBox["\[Mu]", 
         RowBox[{"n", "-", "p", " "}]], 
        SubscriptBox["\[Mu]", 
         RowBox[{"n", "-", "p", "+", "1"}]], 
        SubscriptBox["\[Ellipsis]\[Mu]", "n"]}]], " ", 
      SuperscriptBox["g", 
       RowBox[{
        SubscriptBox["\[Mu]", 
         RowBox[{"n", "-", "p", "+", "1"}]], " ", 
        SubscriptBox["\[Nu]", "1"]}]], "\[Ellipsis]", " ", 
      SuperscriptBox["g", 
       RowBox[{
        SubscriptBox["\[Mu]", "n"], " ", 
        SubscriptBox["\[Nu]", "p"]}]], " ", 
      SubscriptBox["w", 
       RowBox[{
        SubscriptBox["\[Nu]", "1"], 
        SubscriptBox["\[Ellipsis]\[Nu]", "p"]}]]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.6308277865703983`*^9, 3.6308279945235233`*^9}, {
   3.630828083964162*^9, 3.630828163027772*^9}, {3.630828625386359*^9, 

      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalGamma]", "[", "a___", "]"}], "**", 
        RowBox[{"\[CapitalGamma]", "[", "b___", "]"}]}], "\[Rule]", 
       RowBox[{"\[CapitalGamma]", "[", 
        RowBox[{"a", ",", "b"}], "]"}]}]}], "]"}]}], "\[IndentingNewLine]", 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{

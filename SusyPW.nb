Cell["See Green-Schwarz-Witten eq.(13.1.26).", "Text",
 CellChangeTimes->{{3.64225072648431*^9, 3.642250755906623*^9}}],

Cell[TextData[{
 "Gravitino \[Psi] (spin 3/2) and dilatino \[Lambda] (spin 1/2) :\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox["\[Delta]\[Psi]", "M"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["D", "M"], "+", 
        RowBox[{
         FractionBox["\[ImaginaryI]", 
          RowBox[{"4", " ", 
           RowBox[{"5", "!"}]}]], 
         SubscriptBox["F", "PQRST"], 
         SuperscriptBox["\[Gamma]", "PQRST"], 
         SubscriptBox["\[Gamma]", "M"]}]}], ")"}], "\[Epsilon]"}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"4", " ", 
        RowBox[{"4", "!"}]}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["\[Gamma]", "M"], "PQR"], "-", 
        RowBox[{"9", 
         SuperscriptBox[
          SubscriptBox["\[Delta]", "M"], "P"], 
         SuperscriptBox["\[Gamma]", "QR"]}]}], ")"}], 
      SubscriptBox["G", "PQR"], 
      SuperscriptBox["\[Epsilon]", "*"]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", 
    RowBox[{
     SubscriptBox["D", "M"], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["\[PartialD]", "M"], 
        RowBox[{"+", 
         FractionBox["1", "4"]}]}], 
       SubscriptBox["\[Omega]", "MPQ"], 
       SuperscriptBox["\[Gamma]", "PQ"]}], "-", 
      FractionBox["\[ImaginaryI]", "2"]}]}], " "}], TraditionalForm], "\n", 
  FormBox[
   RowBox[{"\[Delta]\[Lambda]", "=", 
    RowBox[{"\[ImaginaryI]", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["P", "M"], 
        SuperscriptBox["\[Gamma]", "M"], 
        SuperscriptBox["\[Epsilon]", "*"]}], "-", 
       RowBox[{
        FractionBox["1", 
         RowBox[{"4", "!"}]], 
        SubscriptBox["G", "MNP"], 
        SuperscriptBox["\[Gamma]", "MNP"], "\[Epsilon]"}]}], ")"}]}]}], 
   TraditionalForm]}]],
 "\n\[Epsilon] is a complex chiral spinor such that (Weyl spinor):\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Gamma]", "11"], "\[Epsilon]"}], "=", 
    RowBox[{"-", "\[Epsilon]"}]}], TraditionalForm]]],
 "\nUnbroken susy requires:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Delta]\[Psi]", "M"], "=", 
    RowBox[{
     RowBox[{"0", " ", "&&", " ", "\[Delta]\[Lambda]"}], "=", "0"}]}], 
   TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.64215906740926*^9, 3.642159271884419*^9}, {
  3.642159308540554*^9, 3.64215934244655*^9}, {3.642159394319538*^9, 
